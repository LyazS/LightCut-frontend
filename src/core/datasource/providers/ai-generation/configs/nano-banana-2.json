{
  "id": "nano-banana-2",
  "name": {
    "en": "Nano Banana 2",
    "zh": "Nano Banana 2"
  },
  "description": {
    "en": "Edit images using NanoBananaPro model, supports multiple images (Max 14)",
    "zh": "使用NanoBananaPro模型进行图片编辑，最多支持14张图片输入"
  },
  "contentType": "image",
  "aiTaskType": "bizyair_generate_media",
  "uploadServer": "bizyair",
  "cost": 0.2,
  "aiConfig": {
    "ref_images": [],
    "prompt": "",
    "seed": -1,
    "aspect_ratio": "1:1",
    "resolution": "1K"
  },
  "uiConfig": [
    {
      "type": "file-input",
      "label": {
        "en": "Images",
        "zh": "图片"
      },
      "path": "aiConfig.ref_images",
      "accept": ["image"],
      "maxFiles": 14,
      "minFiles": 0,
      "placeholder": {
        "en": "Drag image here or from timeline",
        "zh": "拖拽图片素材或时间轴片段到此处"
      }
    },
    {
      "type": "textarea-input",
      "label": {
        "en": "Prompt",
        "zh": "提示词"
      },
      "path": "aiConfig.prompt",
      "placeholder": {
        "en": "Describe how to edit the images.",
        "zh": "描述如何编辑图片"
      }
    },
    {
      "type": "select-input",
      "label": {
        "en": "Aspect Ratio",
        "zh": "宽高比"
      },
      "path": "aiConfig.aspect_ratio",
      "options": [
        {
          "label": {
            "en": "1:1",
            "zh": "1:1"
          },
          "value": "1:1"
        },
        {
          "label": {
            "en": "16:9",
            "zh": "16:9"
          },
          "value": "16:9"
        },
        {
          "label": {
            "en": "9:16",
            "zh": "9:16"
          },
          "value": "9:16"
        },
        {
          "label": {
            "en": "4:3",
            "zh": "4:3"
          },
          "value": "4:3"
        },
        {
          "label": {
            "en": "3:4",
            "zh": "3:4"
          },
          "value": "3:4"
        }
      ]
    },
    {
      "type": "select-input",
      "label": {
        "en": "Resolution",
        "zh": "分辨率"
      },
      "path": "aiConfig.resolution",
      "options": [
        {
          "label": {
            "en": "1K",
            "zh": "1K"
          },
          "value": "1K"
        },
        {
          "label": {
            "en": "2K",
            "zh": "2K"
          },
          "value": "2K"
        },
        {
          "label": {
            "en": "4K",
            "zh": "4K"
          },
          "value": "4K"
        }
      ]
    },
    {
      "type": "number-input",
      "label": {
        "en": "Seed",
        "zh": "随机种子"
      },
      "path": "aiConfig.seed",
      "min": -1,
      "max": 999999999999999,
      "step": 1,
      "precision": 0,
      "placeholder": {
        "en": "-1 for random seed",
        "zh": "-1 表示随机生成"
      }
    }
  ]
}