{
  "id": "sora2-hd",
  "name": {
    "en": "Sora2 video + FlashVSR Upscaling",
    "zh": "Sora2 视频 + FlashVSR 超分"
  },
  "description": {
    "en": "Sora2 video + FlashVSR Upscaling",
    "zh": "使用 Sora2 模型生成视频，并通过 FlashVSR 模型进行超分辨率处理，提升视频质量。"
  },
  "contentType": "video",
  "aiTaskType": "runninghub_generate_media",
  "uploadServer": "runninghub",
  "subAiTaskType": "comfyui",
  "cost": 1.0,
  "aiConfig": {
    "image": [],
    "prompt": "",
    "model": "portrait",
    "duration_seconds": 10
  },
  "uiConfig": [
    {
      "type": "file-input",
      "label": {
        "en": "Image",
        "zh": "图片"
      },
      "path": "aiConfig.image",
      "accept": ["image"],
      "maxFiles": 1,
      "minFiles": 0,
      "placeholder": {
        "en": "Drag image here or from timeline",
        "zh": "拖拽图片素材或时间轴片段到此处"
      }
    },
    {
      "type": "textarea-input",
      "label": {
        "en": "Prompt",
        "zh": "提示词"
      },
      "path": "aiConfig.prompt",
      "placeholder": {
        "en": "Enter prompt to generate video",
        "zh": "输入提示词来生成视频"
      },
      "required": true
    },
    {
      "type": "select-input",
      "label": {
        "en": "Model",
        "zh": "视频比例"
      },
      "path": "aiConfig.model",
      "options": [
        {
          "label": {
            "en": "Portrait",
            "zh": "竖屏"
          },
          "value": "portrait"
        },
        {
          "label": {
            "en": "Landscape",
            "zh": "横屏"
          },
          "value": "landscape"
        }
      ],
      "required": true
    },
    {
      "type": "select-input",
      "label": {
        "en": "Duration",
        "zh": "视频时长"
      },
      "path": "aiConfig.duration_seconds",
      "options": [
        {
          "label": {
            "en": "10 seconds",
            "zh": "10秒"
          },
          "value": 10
        },
        {
          "label": {
            "en": "15 seconds",
            "zh": "15秒"
          },
          "value": 15
        }
      ],
      "required": true
    }
  ]
}
