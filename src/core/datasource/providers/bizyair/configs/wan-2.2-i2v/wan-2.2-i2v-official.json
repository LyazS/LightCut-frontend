{
  "id": "wan-2.2-i2v",
  "variant": "official",
  "web_app_id": 45742,
  "name": "wan-2.2-i2v-official",
  "description": "Wan 2.2 图生视频 - 官方模型+4步LoRA",
  "media_type": "video",
  "cost": 0.5,
  "input_mapping": {
    "ref_images": {
      "type": "array",
      "items": [
        {
          "mapping": "106:LoadImage.image",
          "type": "string",
          "default": "",
          "validation": {
            "required": true,
            "description": "参考图片URL"
          }
        }
      ]
    },
    "positive_prompt": {
      "mapping": "16:WanVideoTextEncode.positive_prompt",
      "type": "string",
      "default": "",
      "validation": {
        "required": true,
        "minLength": 1,
        "maxLength": 5000,
        "description": "正面提示词，描述要生成的视频内容"
      }
    },
    "negative_prompt": {
      "mapping": "16:WanVideoTextEncode.negative_prompt",
      "type": "string",
      "default": "色调艳丽，过曝，静态，细节模糊不清，字幕，风格，作品，画作，画面，静止，整体发灰，最差质量，低质量，JPEG压缩残留，丑陋的，残缺的，多余的手指，画得不好的手部，画得不好的脸部，畸形的，毁容的，形态畸形的肢体，手指融合，静止不动的画面，杂乱的背景，三条腿，背景人很多，倒着走",
      "validation": {
        "required": false,
        "maxLength": 5000,
        "description": "负面提示词，描述不希望出现的内容"
      }
    },
    "aspect_ratio": {
      "mapping": "aspect_ratio",
      "type": "option",
      "default": "16:9",
      "validation": {
        "required": false,
        "description": "宽高比（如 16:9, 9:16, 1:1, 4:3, 3:4, 21:9）",
        "option": ["16:9", "9:16", "1:1", "4:3", "3:4", "21:9"]
      },
      "skip_mapping": true
    },
    "width": {
      "mapping": "108:ImageResizeKJv2.width",
      "type": "integer",
      "default": 960,
      "validation": {
        "required": false,
        "min": 256,
        "max": 1920,
        "computed": true,
        "description": "视频宽度（由 aspect_ratio 自动计算）"
      }
    },
    "height": {
      "mapping": "108:ImageResizeKJv2.height",
      "type": "integer",
      "default": 540,
      "validation": {
        "required": false,
        "min": 256,
        "max": 1080,
        "computed": true,
        "description": "视频高度（由 aspect_ratio 自动计算）"
      }
    },
    "duration": {
      "mapping": "112:INTConstant.value",
      "type": "integer",
      "default": 5,
      "validation": {
        "required": false,
        "min": 1,
        "max": 10,
        "description": "视频时长（秒）"
      }
    },
    "seed_1": {
      "mapping": "27:WanVideoSampler.seed",
      "type": "random_seed",
      "default": -1,
      "validation": {
        "required": false,
        "min": -1,
        "max": 999999999999999,
        "description": "随机种子1（-1表示随机生成）"
      }
    },
    "seed_2": {
      "mapping": "90:WanVideoSampler.seed",
      "type": "random_seed",
      "default": -1,
      "validation": {
        "required": false,
        "min": -1,
        "max": 999999999999999,
        "description": "随机种子2（-1表示随机生成）"
      }
    }
  }
}
