{
  "id": "nano-banana-2",
  "variant": "default",
  "web_app_id": 44130,
  "name": "nano-banana-2",
  "description": "使用NanoBananaPro模型进行图片编辑，支持多张图片输入，根据提示词修改图片内容",
  "media_type": "image",
  "cost": 0.2,
  "real_cost": 0.15,
  "input_mapping": {
    "ref_images": {
      "mapping": "22:LoadImagesFromURL.url",
      "type": "arrayurl",
      "default": [],
      "separator": "\n",
      "validation": {
        "required": true,
        "minLength": 1,
        "description": "参考图片URL列表"
      }
    },
    "prompt": {
      "mapping": "18:BizyAir_NanoBananaProOfficial.prompt",
      "type": "string",
      "default": "将图1人物裙子贴上图2的图案",
      "validation": {
        "required": true,
        "minLength": 1,
        "maxLength": 5000,
        "description": "编辑提示词，描述如何修改图片"
      }
    },
    "seed": {
      "mapping": "18:BizyAir_NanoBananaProOfficial.seed",
      "type": "random_seed",
      "default": -1,
      "validation": {
        "required": false,
        "min": -1,
        "max": 999999999999999,
        "description": "随机种子（-1表示随机生成）"
      }
    },
    "aspect_ratio": {
      "mapping": "18:BizyAir_NanoBananaProOfficial.aspect_ratio",
      "type": "option",
      "default": "1:1",
      "validation": {
        "required": false,
        "description": "宽高比（如 1:1, 16:9, 9:16）",
        "option": [
          "4:3",
          "3:4",
          "16:9",
          "9:16",
          "2:3",
          "3:2",
          "1:1",
          "4:5",
          "5:4",
          "21:9"
        ]
      }
    },
    "resolution": {
      "mapping": "18:BizyAir_NanoBananaProOfficial.resolution",
      "type": "option",
      "default": "1K",
      "validation": {
        "required": false,
        "description": "分辨率（如 1K, 2K, 4K）",
        "option": ["1K", "2K", "4K"],
        "add_cost": {
          "1K": 0,
          "2K": 0.05,
          "4K": 0.1
        },
        "add_real_cost": {
          "1K": 0,
          "2K": 0.0,
          "4K": 0.0
        }
      }
    }
  }
}
