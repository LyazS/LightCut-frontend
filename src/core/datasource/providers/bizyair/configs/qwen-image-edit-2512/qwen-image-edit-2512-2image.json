{
  "id": "qwen-image-edit-2512",
  "variant": "2image",
  "web_app_id": 43062,
  "name": "qwen-image-edit-2512-2image",
  "description": "将两个图片进行合成，根据提示词将图2的元素添加到图1中",
  "media_type": "image",
  "cost": 0.05,
  "input_mapping": {
    "ref_images": {
      "type": "array",
      "items": [
        {
          "mapping": "41:LoadImage.image",
          "type": "string",
          "default": "",
          "validation": {
            "required": true,
            "description": "第一张图片URL（背景图）"
          }
        },
        {
          "mapping": "79:LoadImage.image",
          "type": "string",
          "default": "",
          "validation": {
            "required": true,
            "description": "第二张图片URL（要合成的元素）"
          }
        }
      ]
    },
    "prompt": {
      "mapping": "117:Text Multiline.text",
      "type": "string",
      "default": "将图2的玩偶趴在在图1的后背",
      "validation": {
        "required": true,
        "minLength": 1,
        "maxLength": 5000,
        "description": "合成提示词，描述如何将图2的元素添加到图1中"
      }
    },
    "negative_prompt": {
      "mapping": "118:Text Multiline.text",
      "type": "string",
      "default": "",
      "validation": {
        "required": false,
        "maxLength": 5000,
        "description": "负面提示词（可选）"
      }
    },
    "width": {
      "mapping": "119:PrimitiveInt.value",
      "type": "integer",
      "default": 1024,
      "validation": {
        "required": false,
        "min": 256,
        "max": 2048,
        "step": 8,
        "description": "输出图片宽度（必须是 8 的倍数）"
      }
    },
    "height": {
      "mapping": "120:PrimitiveInt.value",
      "type": "integer",
      "default": 1024,
      "validation": {
        "required": false,
        "min": 256,
        "max": 2048,
        "step": 8,
        "description": "输出图片高度（必须是 8 的倍数）"
      }
    },
    "seed": {
      "mapping": "65:KSampler.seed",
      "type": "random_seed",
      "default": -1,
      "validation": {
        "required": false,
        "min": -1,
        "max": 999999999999999,
        "description": "随机种子（-1表示随机生成）"
      }
    }
  }
}
