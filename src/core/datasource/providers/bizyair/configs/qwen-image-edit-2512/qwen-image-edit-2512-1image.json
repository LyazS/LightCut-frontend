{
  "id": "qwen-image-edit-2512",
  "variant": "1image",
  "web_app_id": 43061,
  "name": "qwen-image-edit-2512-1image",
  "description": "对单张图片进行编辑，根据提示词修改图片内容",
  "media_type": "image",
  "cost": 0.05,
  "input_mapping": {
    "ref_images": {
      "type": "array",
      "items": [
        {
          "mapping": "41:LoadImage.image",
          "type": "string",
          "default": "",
          "validation": {
            "required": true,
            "description": "图片URL（要编辑的图片）"
          }
        }
      ]
    },
    "prompt": {
      "mapping": "117:Text Multiline.text",
      "type": "string",
      "default": "将图中白色裙子换成银色的",
      "validation": {
        "required": true,
        "minLength": 1,
        "maxLength": 5000,
        "description": "编辑提示词，描述如何修改图片"
      }
    },
    "negative_prompt": {
      "mapping": "118:Text Multiline.text",
      "type": "string",
      "default": "",
      "validation": {
        "required": false,
        "maxLength": 5000,
        "description": "负面提示词（可选）"
      }
    },
    "width": {
      "mapping": "119:PrimitiveInt.value",
      "type": "integer",
      "default": 1024,
      "validation": {
        "required": false,
        "min": 256,
        "max": 2048,
        "step": 8,
        "description": "输出图片宽度（必须是 8 的倍数）"
      }
    },
    "height": {
      "mapping": "120:PrimitiveInt.value",
      "type": "integer",
      "default": 1024,
      "validation": {
        "required": false,
        "min": 256,
        "max": 2048,
        "step": 8,
        "description": "输出图片高度（必须是 8 的倍数）"
      }
    },
    "seed": {
      "mapping": "65:KSampler.seed",
      "type": "random_seed",
      "default": -1,
      "validation": {
        "required": false,
        "min": -1,
        "max": 999999999999999,
        "description": "随机种子（-1表示随机生成）"
      }
    }
  }
}