{
  "id": "qwen3-tts-clone",
  "name": {
    "en": "Qwen3 Voice Clone",
    "zh": "Qwen3语音克隆"
  },
  "description": {
    "en": "Qwen3 Voice Clone - Clone voice from reference audio and generate new speech",
    "zh": "Qwen3语音克隆 - 根据参考音频克隆声音并生成新的语音"
  },
  "contentType": "audio",
  "aiTaskType": "bizyair_generate_media",
  "uploadServer": "bizyair",
  "cost": 0.2,
  "aiConfig": {
    "ref_audio": {
      "type": "array",
      "value": []
    },
    "ref_text": {
      "type": "string",
      "value": ""
    },
    "target_text": {
      "type": "string",
      "value": "我真的太高兴了"
    }
  },
  "uiConfig": [
    {
      "type": "file-input",
      "label": {
        "en": "Reference Audio",
        "zh": "参考音频"
      },
      "path": "aiConfig.ref_audio",
      "accept": ["audio"],
      "maxFiles": 1,
      "minFiles": 1,
      "placeholder": {
        "en": "Drag audio file here or from timeline",
        "zh": "拖拽音频素材或时间轴片段到此处"
      }
    },
    {
      "type": "textarea-input",
      "label": {
        "en": "Reference Text",
        "zh": "参考文本"
      },
      "path": "aiConfig.ref_text",
      "placeholder": {
        "en": "Enter the text corresponding to the reference audio (required for accurate cloning)...",
        "zh": "输入参考音频对应的文本（必须与音频内容对应）..."
      },
      "minRows": 2,
      "maxRows": 5,
      "required": true
    },
    {
      "type": "textarea-input",
      "label": {
        "en": "Target Text",
        "zh": "目标文本"
      },
      "path": "aiConfig.target_text",
      "placeholder": {
        "en": "Enter the text to generate...",
        "zh": "输入要生成的文本内容..."
      },
      "minRows": 3,
      "maxRows": 8
    }
  ]
}
