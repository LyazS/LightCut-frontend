{
  "id": "rh-sora2",
  "name": {
    "en": "Super Openai Rendering Algorithm :)",
    "zh": "CloseAI-S2 视频生成模型"
  },
  "description": {
    "en": "All-in-One Video from CloseAI - Text to Video / Image to Video",
    "zh": "来自 CloseAI 的全能视频 - 支持文生视频和图生视频"
  },
  "contentType": "video",
  "aiTaskType": "runninghub_generate_media",
  "subAiTaskType": "standard_api",
  "uploadServer": "runninghubstd",
  "cost": 0.1,
  "aiConfig": {
    "imageUrls": [],
    "prompt": "",
    "aspectRatio": "16:9",
    "duration": "10"
  },
  "uiConfig": [
    {
      "type": "file-input",
      "label": {
        "en": "Image (Optional)",
        "zh": "图片（可选）"
      },
      "path": "aiConfig.imageUrls",
      "accept": ["image"],
      "maxFiles": 1,
      "minFiles": 0,
      "placeholder": {
        "en": "Drag image here for image-to-video, or leave empty for text-to-video",
        "zh": "拖拽图片进行图生视频，留空则进行文生视频"
      }
    },
    {
      "type": "textarea-input",
      "label": {
        "en": "Prompt",
        "zh": "提示词"
      },
      "path": "aiConfig.prompt",
      "placeholder": {
        "en": "Describe the video you want to generate...",
        "zh": "描述你想生成的视频内容..."
      },
      "required": true,
      "maxLength": 5000
    },
    {
      "type": "select-input",
      "label": {
        "en": "Aspect Ratio",
        "zh": "宽高比"
      },
      "path": "aiConfig.aspectRatio",
      "options": [
        {
          "label": {
            "en": "16:9",
            "zh": "16:9"
          },
          "value": "16:9"
        },
        {
          "label": {
            "en": "9:16",
            "zh": "9:16"
          },
          "value": "9:16"
        }
      ]
    },
    {
      "type": "select-input",
      "label": {
        "en": "Duration",
        "zh": "视频时长"
      },
      "path": "aiConfig.duration",
      "options": [
        {
          "label": {
            "en": "10 seconds",
            "zh": "10秒"
          },
          "value": "10"
        },
        {
          "label": {
            "en": "15 seconds",
            "zh": "15秒"
          },
          "value": "15",
          "add_cost": 0.1
        }
      ]
    }
  ]
}
