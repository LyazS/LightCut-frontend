{
  "id": "wan-2.2-i2v",
  "name": {
    "en": "Wan 2.2 Image to Video",
    "zh": "Wan 2.2 图生视频"
  },
  "description": {
    "en": "Generate videos from images using Wan 2.2 model with multiple variants",
    "zh": "使用 Wan 2.2 模型从图片生成视频，支持多个变种"
  },
  "contentType": "video",
  "aiTaskType": "bizyair_generate_media",
  "uploadServer": "bizyair",
  "cost": 0.5,
  "aiConfig": {
    "variant": {
      "type": "string",
      "value": "smoothv2"
    },
    "ref_images": {
      "type": "array",
      "value": []
    },
    "prompt": {
      "type": "string",
      "value": ""
    },
    "negative_prompt": {
      "type": "string",
      "value": "色调艳丽，过曝，静态，细节模糊不清，字幕，风格，作品，画作，画面，静止，整体发灰，最差质量，低质量，JPEG压缩残留，丑陋的，残缺的，多余的手指，画得不好的手部，画得不好的脸部，畸形的，毁容的，形态畸形的肢体，手指融合，静止不动的画面，杂乱的背景，三条腿，背景人很多，倒着走"
    },
    "aspect_ratio": {
      "type": "string",
      "value": "16:9"
    },
    "duration": {
      "type": "number",
      "value": 5
    },
    "seed_1": {
      "type": "number",
      "value": -1
    },
    "seed_2": {
      "type": "number",
      "value": -1
    }
  },
  "uiConfig": [
    {
      "type": "file-input",
      "label": {
        "en": "Image",
        "zh": "图片"
      },
      "path": "aiConfig.ref_images",
      "accept": ["image"],
      "maxFiles": 1,
      "minFiles": 1,
      "placeholder": {
        "en": "Drag image here or from timeline",
        "zh": "拖拽图片素材或时间轴片段到此处"
      }
    },
    {
      "type": "select-input",
      "label": {
        "en": "Model Variant",
        "zh": "模型变种"
      },
      "path": "aiConfig.variant",
      "options": [
        {
          "value": "smoothv2",
          "label": {
            "en": "Smooth v2 (Recommended)",
            "zh": "Smooth v2（推荐）"
          }
        },
        {
          "value": "smoothv1",
          "label": {
            "en": "Smooth v1",
            "zh": "Smooth v1"
          }
        },
        {
          "value": "official",
          "label": {
            "en": "Official + 4-step LoRA",
            "zh": "官方模型 + 4步LoRA"
          }
        }
      ]
    },
    {
      "type": "textarea-input",
      "label": {
        "en": "Prompt",
        "zh": "提示词"
      },
      "path": "aiConfig.prompt",
      "placeholder": {
        "en": "Describe the video content you want to generate...",
        "zh": "描述你想要生成的视频内容..."
      },
      "required": true,
      "maxLength": 5000,
      "enableTag": true
    },
    {
      "type": "textarea-input",
      "label": {
        "en": "Negative Prompt",
        "zh": "负面提示词"
      },
      "path": "aiConfig.negative_prompt",
      "placeholder": {
        "en": "Describe what you don't want in the video...",
        "zh": "描述你不希望在视频中出现的内容..."
      },
      "maxLength": 5000
    },
    {
      "type": "select-input",
      "label": {
        "en": "Aspect Ratio",
        "zh": "宽高比"
      },
      "path": "aiConfig.aspect_ratio",
      "options": [
        {
          "value": "16:9",
          "label": {
            "en": "16:9 (Landscape - 960x540)",
            "zh": "16:9 (横屏 - 960x540)"
          }
        },
        {
          "value": "9:16",
          "label": {
            "en": "9:16 (Portrait - 540x960)",
            "zh": "9:16 (竖屏 - 540x960)"
          }
        },
        {
          "value": "1:1",
          "label": {
            "en": "1:1 (Square - 720x720)",
            "zh": "1:1 (方形 - 720x720)"
          }
        },
        {
          "value": "4:3",
          "label": {
            "en": "4:3 (Landscape - 960x720)",
            "zh": "4:3 (横屏 - 960x720)"
          }
        },
        {
          "value": "3:4",
          "label": {
            "en": "3:4 (Portrait - 720x960)",
            "zh": "3:4 (竖屏 - 720x960)"
          }
        },
        {
          "value": "21:9",
          "label": {
            "en": "21:9 (Ultrawide - 1260x540)",
            "zh": "21:9 (超宽屏 - 1260x540)"
          }
        }
      ]
    },
    {
      "type": "number-input",
      "label": {
        "en": "Duration",
        "zh": "时长"
      },
      "path": "aiConfig.duration",
      "min": 1,
      "max": 10,
      "step": 1,
      "precision": 0,
      "placeholder": {
        "en": "Video duration (seconds)",
        "zh": "视频时长（秒）"
      }
    },
    {
      "type": "number-input",
      "label": {
        "en": "Seed 1",
        "zh": "随机种子1"
      },
      "path": "aiConfig.seed_1",
      "min": -1,
      "max": 999999999999999,
      "step": 1,
      "precision": 0,
      "placeholder": {
        "en": "-1 for random seed",
        "zh": "-1 表示随机生成"
      }
    },
    {
      "type": "number-input",
      "label": {
        "en": "Seed 2",
        "zh": "随机种子2"
      },
      "path": "aiConfig.seed_2",
      "min": -1,
      "max": 999999999999999,
      "step": 1,
      "precision": 0,
      "placeholder": {
        "en": "-1 for random seed",
        "zh": "-1 表示随机生成"
      }
    }
  ]
}
