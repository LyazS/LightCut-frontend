{
  "id": "qwen-image-edit-2512",
  "name": {
    "en": "Qwen Image Edit 2511",
    "zh": "Qwen 图片编辑 2511 版本"
  },
  "description": {
    "en": "Edit 1/2/3 images based on prompt words",
    "zh": "根据提示词编辑1/2/3张图片"
  },
  "contentType": "image",
  "aiTaskType": "bizyair_generate_media",
  "uploadServer": "bizyair",
  "cost": 0.05,
  "aiConfig": {
    "ref_images": [],
    "prompt": "",
    "negative_prompt": "",
    "width": 1024,
    "height": 1024,
    "seed": -1
  },
  "uiConfig": [
    {
      "type": "file-input",
      "label": {
        "en": "Images",
        "zh": "图片"
      },
      "path": "aiConfig.ref_images",
      "accept": ["image"],
      "maxFiles": 3,
      "minFiles": 1,
      "placeholder": {
        "en": "Drag image here or from timeline",
        "zh": "拖拽图片素材或时间轴片段到此处"
      }
    },
    {
      "type": "textarea-input",
      "label": {
        "en": "Prompt",
        "zh": "提示词"
      },
      "path": "aiConfig.prompt",
      "placeholder": {
        "en": "Describe how to edit the images.",
        "zh": "描述如何编辑图片"
      }
    },
    {
      "type": "textarea-input",
      "label": {
        "en": "Negative Prompt",
        "zh": "负面提示词"
      },
      "path": "aiConfig.negative_prompt",
      "placeholder": {
        "en": "Enter negative prompt (optional)...",
        "zh": "输入负面提示词（可选）..."
      },
      "required": false
    },
    {
      "type": "number-input",
      "label": {
        "en": "Width",
        "zh": "宽度"
      },
      "path": "aiConfig.width",
      "min": 256,
      "max": 2048,
      "step": 8,
      "precision": 0
    },
    {
      "type": "number-input",
      "label": {
        "en": "Height",
        "zh": "高度"
      },
      "path": "aiConfig.height",
      "min": 256,
      "max": 2048,
      "step": 8,
      "precision": 0
    },
    {
      "type": "number-input",
      "label": {
        "en": "Seed",
        "zh": "随机种子"
      },
      "path": "aiConfig.seed",
      "min": -1,
      "max": 999999999999999,
      "step": 1,
      "precision": 0,
      "placeholder": {
        "en": "-1 for random seed",
        "zh": "-1 表示随机生成"
      }
    }
  ]
}
